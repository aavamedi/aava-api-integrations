{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "lib": ["es2017", "dom"],
    "sourceMap": true,
    "noEmitOnError": true,
    "strict": true,
    "noImplicitAny": false, // declare module "@okta/jwt-verifier" not working?
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "strictFunctionTypes": true,
    "esModuleInterop": true,
    "preserveWatchOutput": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}
